        {
            "before": ["<leader>", "<space>"],
            "commands": ["workbench.action.showCommands"]
        },
        {
            "before": ["<leader>", ";"],
            "commands": ["editor.action.commentLine"]
        },
        {
            "before": ["f", "d"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<leader>", "j", "="],
            "commands": ["editor.action.format"]
        },
        {
            "before": ["leader", "j", "j"],
            "after": ["leader", "leader", "s"]
        },
        {
            "before": ["leader", "j", "l"],
            "after": [
                "leader",
                "leader",
                "leader",
                "b",
                "d",
                "j",
                "k"
            ]
        },
        {
            "before": ["leader", "j", "w"],
            "after": [
                "leader",
                "leader",
                "leader",
                "b",
                "d",
                "w"
            ]
        }
    ],
